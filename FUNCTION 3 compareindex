int compIndex(char str1[], char str2[])
{
   if(strlen(str1)>strlen(str2))
     return 1;
    else if(strlen(str1)>strlen(str2))
     return 0;
     else(strlen(str1)>strlen(str2))
     return -1;
 }  
 
 int main()
 {
   char str1[100],str2[100];
   scanf("%c  %c", str1, str2);
   printf("%d", compIndex(str1,str2); // control string is %d because return is int
   )
   
