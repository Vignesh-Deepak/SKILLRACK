#include<stdio.h>
#include<stdlib.h>

int main()
{
    char str[1001];
    fgets(str,sizeof(str),stdin);
    for(int i=0;str[i]!='\0';i++)
    {
        if(isalpha(str[i]))
          printf("%c",str[i]);
          
    }
    for(int i=0; str[i]!='\0';i++)
    {
        if(isdigit(str[i]))
          printf("%s",str[i]);
          
    }
    for(int i=0; str[i]!='\0';i++)
    {
        if(!isalpha(str[i])&& !isdigit(str[i])     // if(isalpha(str[i])==0 && (isdigit(str[i]==0)
         printf("%s",str[i]);
    }
    

}


USING FUNCTIONS
char checker(char *str)
{
   for(int i=0;str[i]!='\0';i++)
    {
        if(isalpha(str[i]))
          printf("%c",str[i]);
          
    }
    for(int i=0; str[i]!='\0';i++)
    {
        if(isdigit(str[i]))
          printf("%s",str[i]);
          
    }
    for(int i=0; str[i]!='\0';i++)
    {
        if(!isalpha(str[i])&& !isdigit(str[i])     // if(isalpha(str[i])==0 && (isdigit(str[i]==0)
         printf("%s",str[i]);
    }
    return checker;


int main()
{
  char str[1001];
  fgets(str,1001,stdin);
  printf("%s",checker(str));
  }
  
