#include<stdio.h>
#include<stdlib.h>

int main()
{
    int N,no_element=0;
    scanf("%d",&N);
    int arr1[N], arr2[N];
    for(int i=0;i<=N-1;i++)
    {
        scanf("%d", &arr1[i]);
    }
    for(int i=0;i<N;i++)
    {
        scanf("%d", &arr2[i]);
    }
    for(int i=0;i<=N-1;i++)
    {
        if(arr1[i] == arr2[N-i-1]){
        printf("%d", arr1[i]);
        no_element=1;}
    }
    if(no_element==0){
        
     printf("-1");
    }
    return 0;

}
